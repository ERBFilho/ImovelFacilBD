            <TextField fx:id="celularEntrada" maxHeight="-Infinity" maxWidth="320.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="320.0" promptText="CELULAR" />
            <TextField fx:id="telefoneEntrada" maxHeight="-Infinity" maxWidth="320.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="320.0" promptText="TELEFONE" />
            <TextField fx:id="emailEntrada" maxHeight="-Infinity" maxWidth="320.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="320.0" promptText="EMAIL" />
