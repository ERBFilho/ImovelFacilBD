               <children>
                  <HBox alignment="CENTER" prefHeight="43.0" prefWidth="394.0" spacing="10.0">
                     <children>
                        <TextField fx:id="cpfEntrada" maxHeight="-Infinity" maxWidth="320.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" promptText="CPF" />
                        <TextField fx:id="cnpjEntrada" maxHeight="-Infinity" maxWidth="320.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" promptText="CNPJ" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="43.0" prefWidth="394.0" spacing="10.0">
                     <children>
                        <TextField fx:id="sexoEntrada" maxHeight="-Infinity" maxWidth="320.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" promptText="SEXO" />
                        <TextField fx:id="rSocialEntrada" maxHeight="-Infinity" maxWidth="320.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" promptText="RAZÃO SOCIAL" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets top="10.0" />
               </padding>
            </VBox>
            <HBox alignment="CENTER" prefHeight="76.0" prefWidth="394.0" spacing="20.0">
               <children>
                  <ImageView fx:id="butCancelar" fitHeight="57.0" fitWidth="177.0" onMousePressed="#cancelarBut" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../imagens/cadastro/cancelarBut.png" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="butSalvar" fitHeight="57.0" fitWidth="158.0" onMousePressed="#salvar" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../imagens/cadastro/salvarBut.png" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
